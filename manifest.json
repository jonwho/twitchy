{
  "manifest_version": 2,
  "name": "Twitchy",
  "version": "0.0.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js","content.js"]
    }
  ],
  "browser_action": {
     "default_popup": "popup.html"
   },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab"
  ]
}
